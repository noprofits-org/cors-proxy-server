{
    "version": 2,
    "functions": {
      "api/proxy.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/proxy.js",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, PATCH, DELETE",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
        }
      }
    ]
  }