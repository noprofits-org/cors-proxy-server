{
  "version": 2,
  "routes": [
    { 
      "src": "/api/(.*)", 
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "X-API-Key, X-Requested-With, Content-Type, Accept"
      },
      "continue": true
    },
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/env-test", "dest": "/api/env-test.js" },
    { "src": "/api/proxy", "dest": "/api/proxy.js" },
    { "src": "/api/logs", "dest": "/api/logs.js" },
    
    { "handle": "filesystem" },
    
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}